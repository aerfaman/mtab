{
	"manifest_version":2,
	"name":"Mtab",
	"version":"0.1",
	// "background":{"scripts":["js/main.js"]},
	"permissions":[
		"tabs","storage",
		"pageCapture"
	],

	"description":"Save every tab you want.",
	"icons":{
		"16":"images/logo/logo16.png",
		"48":"images/logo/logo48.png",
		"128":"images/logo/logo128.png"
	},
	"browser_action":{
		"default_icon":{
			"19":"images/logo/logo19.png",
			"38":"images/logo/logo38.png"
		},
		"default_title":"Mtab",
		"default_popup":"popup.html"
	},
	"options_page":"index.html"

}
